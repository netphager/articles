<div id="article">

    <label>Title</label>
    <input type="text" name="title" value="{{article.title}}" />
    <br />
    <label>Text</label>
    <textarea name="text">{{article.text}}</textarea>
    <br />
    <a href="/app/#/article/update/id/{{article._id}}" class="saveArticle">Save</a>
</div>